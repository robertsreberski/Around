<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
                android:layout_height="match_parent"
        >
    <RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent"
                    android:id="@+id/main_container">
        <com.StrapleGroup.around.ui.utils.AroundViewPager

                android:id="@+id/pager"
                android:layout_width="match_parent"
                android:layout_height="match_parent"

                android:layout_alignParentTop="true" android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" android:layout_alignParentBottom="true"/>
    </RelativeLayout>
        <ImageButton
                android:layout_width="65dp"
                android:layout_height="65dp"
                android:id="@+id/drawer" android:layout_alignParentBottom="true"
                android:onClick="menu"
                android:background="@drawable/circle_background"
                android:elevation="4dp"
                android:layout_alignParentRight="true" android:layout_alignParentEnd="true"
                android:layout_marginBottom="15dp" android:layout_marginRight="15dp" android:src="@drawable/home_img"/>
</RelativeLayout>